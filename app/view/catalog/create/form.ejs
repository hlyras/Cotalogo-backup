<div id="catalog-create-container" class="box b1 container">
  <div class="mobile-box b10 center"></div>
  <div class="mobile-box b4-5 em12 lucida-grande bold center">Novo Catálogo</div>
  <div class="mobile-box b10 center"><img id="catalog-create-expander" class="size-20 icon noselect"
      src="/images/icon/expand.png"
      onclick="lib.displayDiv('catalog-create-form', this, '/images/icon/expand.png', '/images/icon/retract.png');">
  </div>
  <form id="catalog-create-form" class="box b1 container padding-10" style="display:none;">
    <div class="box b1 lucida-grande em08 bold margin-top-10">Nome do catálogo</div>
    <input type="hidden" name="id" value="">
    <input type="text" name="name" class="mobile-box b5-6 input-generic border-lg-st radius-5 center nofocus"
      placeholder="Nome do Catálogo" autocomplete="off" maxlength="100">
    <img class='mobile-box size-20 center icon pointer noselect' onclick="lib.display('code-help', '')"
      src="/images/icon/help.png">

    <div class="box b1 lucida-grande em08 bold margin-top-10">Endereço da URL</div>
    <div class="mobile-box b3 lucida-grande em12 bold center">suaempresa/</div>
    <input type="text" name="url" class="mobile-box b2-3 lucida-grande em12 bold border-bottom padding-5 nofocus"
      placeholder="novocatalogo" autocomplete="off" maxlength="100">

    <input type="submit" name="submit"
      class="box b3 em10 bold btn-act noborder height-50 center margin-top-20 radius-5 pointer" value="Criar catálogo">
  </form>
</div>
<html>

<head>
    <%- include('./../partials/head.ejs') %>
</head>

<body>
    <%- include('./../partials/userHeader.ejs') %>
    <%- include('./help/variation.ejs') %>
    <%- include('./../partials/nav.ejs') %>
    <section class="container">
        <div class="box b2 container border-explicit ground margin-top-10 padding-10">
            <%- include('./form/category-create.ejs') %>
            <div class="box a1 container border ground margin-top-10 padding-10">
                <div class="box b1 container">
                    <div class="mobile-box b10"></div>
                    <div class="mobile-box b4-5 em12 lucida-grande bold center">Buscar Categorias</div>
                    <div class="mobile-box b10 center"><img class="size-20 icon noselect" src="/images/icon/expand.png"
                            onclick="lib.displayDiv('category-filter-box', this, '/images/icon/expand.png', '/images/icon/retract.png');">
                    </div>
                </div>
                <%- include('./form/category-filter.ejs') %>
            </div>
        </div>

        <div id="category-show-box" class="box b2 container border-explicit ground margin-top-10 padding-10"
            style="display:none">
            <div class="box b1 border padding-10 margin-top-10">
                <div id="category-create-title" class="box b1 container lucida-grande bold center"></div>
                <%- include('./form/variation-create.ejs') %>
            </div>
            <div class="box b1 border padding-10 margin-top-10">
                <div id="category-filter-title" class="box b1 container lucida-grande bold center"></div>
                <%- include('./form/variation-filter.ejs') %>
            </div>
        </div>
    </section>
    <%- include('./../partials/loader.ejs') %>
</body>
<footer>
    <%- include('./../partials/footer.ejs') %>
    <script src="/javascripts/scripts/product/model/category.js"></script>
    <script src="/javascripts/scripts/product/view/category.js"></script>
    <script src="/javascripts/scripts/product/controller/category.js"></script>

    <script src="/javascripts/scripts/product/model/variation.js"></script>
    <script src="/javascripts/scripts/product/view/variation.js"></script>
    <script src="/javascripts/scripts/product/controller/variation.js"></script>
</footer>

</html>